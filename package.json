    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "jsdom": "^21.1.1",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vitest": "^0.23.2"
  }
