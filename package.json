{
  "name": "password-generator-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint:check": "eslint ./src/**/*.*",
    "lint:fix": "eslint --fix ./src/**/*.*",
    "format:check": "prettier ./src",
